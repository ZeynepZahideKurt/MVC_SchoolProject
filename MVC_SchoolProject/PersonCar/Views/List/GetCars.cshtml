@model List<Car>

<h1> Kullanıcı Adı : @Model[0].Person.FirstName  </h1>
<h1> Kullanıcı Soyadı : @Model[0].Person.LastName  </h1>


<table class="table table-hover ">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Brand
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var car in Model)
        {
            <tr>
                <td>
                    @car.Id
                </td>
                <td>
                    @car.Brand
                </td>
            </tr>
        }
    </tbody>

</table>
